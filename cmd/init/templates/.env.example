# ======================================================================
# WARNING: This file is for LOCAL DEVELOPMENT only.
# NEVER use these values in production. In production, use a secrets
# manager and disable dummy auth (DOC_ENGINE_AUTH_DUMMY=false).
# ======================================================================

# Database (must match settings/app.yaml database section)
DOC_ENGINE_DATABASE_HOST=localhost
DOC_ENGINE_DATABASE_PORT=5432
DOC_ENGINE_DATABASE_USER=postgres
DOC_ENGINE_DATABASE_PASSWORD=postgres
DOC_ENGINE_DATABASE_NAME=doc_assembly

# Server
# DOC_ENGINE_SERVER_PORT=8080
# DOC_ENGINE_SERVER_CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Auth (set to false and configure OIDC/Keycloak for production)
DOC_ENGINE_AUTH_DUMMY=true

# Signing (use "mock" for development, "documenso" or others for production)
DOC_ENGINE_SIGNING_PROVIDER=mock
# DOC_ENGINE_SIGNING_API_KEY=
# DOC_ENGINE_SIGNING_BASE_URL=
