# Custom i18n labels and descriptions for injectors.
# These merge with (and override) the built-in translations.
#
# Format:
#   <injector_code>:
#     label:
#       en: "English Label"
#       es: "Etiqueta en Espanol"
#     description:
#       en: "English description"
#       es: "Descripcion en espanol"

example_greeting:
  label:
    en: "Greeting"
    es: "Saludo"
  description:
    en: "A greeting message"
    es: "Un mensaje de saludo"
